{
  "name": "ssr-express-react",
  "version": "1.0.0",
  "description": "Boiler template SSR with Express, React, React router, Helmet, modules CSS and SASS",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/client.dev.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.prod.js --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "start": "cd server && node bin/server.js"
  },
  "author": "Haziel Castilo",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.2",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.0",
    "dotenv-webpack": "^7.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  }
}
